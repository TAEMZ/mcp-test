{
  "name": "mcp-test-kit",
  "version": "0.1.0",
  "description": "Testing framework for MCP (Model Context Protocol) servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./matchers": {
      "types": "./dist/matchers.d.ts",
      "import": "./dist/matchers.mjs",
      "require": "./dist/matchers.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "testing",
    "test",
    "vitest",
    "jest"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "vitest": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
